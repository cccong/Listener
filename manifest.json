{
  "manifest_version": 2,

  "name": "Listener",
  "description": "This extension shows http requests",
  "version": "1.3.2",
  "devtools_page": "devtools.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon/24.png"
  },
  "icons":
  {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "72": "icon/72.png",
    "128": "icon/128.png"
  },
  "permissions": [
    "tabs",
    "webRequest", 
    "webRequestBlocking", 
    "cookies", 
    "*://*/*",
    "contextMenus"
  ]
}
